{
  "name": "@kitzen/assets",
  "version": "1.0.4",
  "author": "kitzen <kitzen.io>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/kitzen-io/assets#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitzen-io/assets.git"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "prebuild-assets": "node src/internal/build.js",
    "prebuild-files": "cp ./package.json lib && cp ./.releaserc.json lib && cp ./tsconfig.json lib && cp ./README.md lib",
    "build": "yarn prebuild && yarn prebuild-assets && yarn prebuild-files",
    "build:watch": "tsc --watch",
    "lint": "eslint 'src/**/*.ts' --fix",
    "lint:check": "eslint 'src/**/*.ts'",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": "./index.js"
  },
  "files": [
    "**/*"
  ],
  "devDependencies": {
    "semantic-release": "^19.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^9.2.0",
    "@semantic-release/npm": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.22.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  }
}
